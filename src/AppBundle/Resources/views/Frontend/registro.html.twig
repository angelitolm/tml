{% block login %}

    <div class="vc_signup block" style="margin-bottom: 35px; margin-top: 35px;">
        <div class="wrapper">
            <div class="container">
                <div class="row">
                    <div class="col-md-7 text-col">
                        <h2> Obtenga nuestro <span class="vc_main-color"> Kit de Herramientas, ya! </span></h2>

