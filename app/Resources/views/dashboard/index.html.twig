{% extends "::admin.html.twig" %}

{% block title %}{{ 'dashboard'|trans({},'backend') }}{% endblock %}
{% block page_title %}{{ 'dashboard'|trans({},'backend') }}{% endblock %}

{% block page_body %}
    <div style="background: #F9F9F9;">
        <!-- BEGIN DASHBOARD STATS 1-->
        <div class="row widget-row">
            <div class="col-md-3">
                <!-- BEGIN WIDGET THUMB -->
                <div class="widget-thumb widget-bg-color-white text-uppercase margin-bottom-20 bordered">
                    <h4 class="widget-thumb-heading">{{ 'bonus.pts' | trans({}, 'backend') }}</h4>

                    <div class="widget-thumb-wrap">
                        <i class="widget-thumb-icon bg-green fa fa-money"></i>

                        <div class="widget-thumb-body">
                            <span class="widget-thumb-subtitle">{{ 'credit.pendient' | trans({}, 'backend') }}</span>
                        <span class="widget-thumb-body-stat" data-counter="counterup"
                              data-value="{{ get_bono(app.user)|number_format(2,'.',',') }}">0</span>
                        </div>
                    </div>
                </div>
                <!-- END WIDGET THUMB -->
            </div>
            <div class="col-md-3">
                <!-- BEGIN WIDGET THUMB -->
                <div class="widget-thumb widget-bg-color-white text-uppercase margin-bottom-20 bordered">
                    <h4 class="widget-thumb-heading">{{ 'credit.pts' | trans({}, 'backend') }}</h4>

                    <div class="widget-thumb-wrap">
                        <i class="widget-thumb-icon bg-red fa fa-usd"></i>

                        <div class="widget-thumb-body">
                            <span class="widget-thumb-subtitle">{{ 'credit.disponible' | trans({}, 'backend') }}</span>
                        <span class="widget-thumb-body-stat" data-counter="counterup"
                              data-value="{{ app.user.getActiveProfile.credit|number_format(2,'.',',') }}">0</span>
                        </div>
                    </div>
                </div>
                <!-- END WIDGET THUMB -->
            </div>

            <div class="col-md-3">
                <!-- BEGIN WIDGET THUMB -->
                <div class="dashboard-stat2 widget-thumb widget-bg-color-white text-uppercase margin-bottom-20 bordered">
                    <h4 class="widget-thumb-heading">{{ 'members.affiliate' | trans({}, 'backend') }}</h4>

                    <div class="widget-thumb-wrap">
                        <i class="widget-thumb-icon bg-purple icon-user-following"></i>

                        <div class="widget-thumb-body">
                            <div class="progress-info">
                                <div class="progress">
                                    {% set cantmemberdescuent = app.user.getActiveProfile.getChildrenMembers|length %}
                                    {#{% set cantmemberdescuent = app.user.getActiveProfile.getChildrenMembersMonth|length  %}#}
                                    <span style="width: {{ cantmemberdescuent }}%;"
                                          class="progress-bar progress-bar-success purple-sharp">
					<span class="sr-only">{{ cantmemberdescuent }}% progress</span>
					</span>
                                </div>
                                <div class="status">
                                    <div class="status-title" style="text-transform: none">
                                        {{ 'affiliate.width.descuent' | trans({}, 'backend') }}
                                    </div>
                                    <div class="status-number">
                                        {{ cantmemberdescuent }}%
                                    </div>
                                    <br><br>

                                    <div class="status-title" style="font-size: 18px">
                                        {{ 'total' | trans({}, 'backend') }}:
                                    <span style="display: inline-block; font-size: 18px" class="widget-thumb-body-stat"
                                          data-counter="counterup"
                                          data-value="{{ app.user.getActiveProfile.getChildrenMembers|length }}">0</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- END WIDGET THUMB -->
            </div>
            <div class="col-md-3">

                <!-- BEGIN WIDGET THUMB -->
                <div class="widget-thumb widget-bg-color-white text-uppercase margin-bottom-20 bordered">
                    <h4 class="widget-thumb-heading">{{ 'members.guests' | trans({}, 'backend') }}</h4>

                    <div class="widget-thumb-wrap" style="overflow: visible">
                        <i class="widget-thumb-icon bg-blue icon-user"></i>

                        <div class="widget-thumb-body" style="overflow: visible">
                            <span class="widget-thumb-subtitle">{{ limitGuest }} {{ 'month' | trans({}, 'backend') }}</span>
                                <span class="widget-thumb-body-stat" data-counter="counterup"
                                      data-value="{{ app.user.getActiveProfile.getGuestChildrenMembers|length }}"
                                      style="display: inline-block">0</span>

                            <div class="btn-group-vertical" style="float: right; z-index: 1000 !important;">
                                <div class="btn-group">
                                    <button id="btnGroupVerticalDrop1" type="button"
                                            class="btn btn-default dropdown-toggle"
                                            data-toggle="dropdown" aria-expanded="false">
                                        {{ 'c.guests' | trans({}, 'backend') }} <i class="fa fa-angle-down"></i>
                                    </button>
                                    <ul class="dropdown-menu" role="menu" aria-labelledby="btnGroupVerticalDrop1"
                                        style="z-index: 1001 !important;">
                                        {% for code_guest in app.user.getGuestCode %}
                                            <li>
                                                <a href="javascript:;">
                                                    {{ code_guest.code }} <span class="badge badge-success"><i
                                                                class="icon-check"></i> </span>
                                                </a>
                                            </li>
                                        {% else %}
                                            <li>
                                                <a href="javascript:;">
                                            {{ 'dont.code' }} </a>
                                            </li>
                                        {% endfor %}
                                    </ul>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
                <!-- END WIDGET THUMB -->
            </div>
        </div>
        <div class="clearfix"></div>
        <!-- END DASHBOARD STATS 1-->

        <div class="row widget-row">

            <div class="col-md-6 margin-bottom-20">
