            </div>


            <div class="col-md-3 col-sm-6 col-xs-12 margin-bottom-20">
                <div class="portlet light bordered" style="background: none repeat scroll 0% 0% #55ACEE;">
                    <div class="portlet-title">
                        <div class="caption caption-md font-white">
                            <i class="icon-shield font-white"></i>
                            <span class="caption-subject theme-font bold uppercase">{{ 'garanties' | trans({}, 'backend') }}</span>
                        </div>

                        <div class="actions">
