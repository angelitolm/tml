                                                 alt="">

                                            <div class="widget-news-right-body">
                                                <h3 class="widget-news-right-body-title"><a
                                                            href="{{ path('news',{'id':news.id}) }}">{{ news.title }}</a>
                                                    <span class="label label-default"> {{ news.created|date('Y-m-d H:i') }} </span>
                                                </h3>

                                                <p>{{ news.body|striptags|slice(0,150) }}</p>
                                            </div><br>
                                        {% else %}
                                            <h2>{{ 'dont.have.news' | trans({}, 'backend') }}</h2>
                                        {% endfor %}
                                    </div>

                                </div>


                            </div>
                        </div>
                        <div class="widget-map-body text-uppercase text-center">
                            <div class="widget-sparkline-chart">
                                <div id="widget_sparkline_bar"></div>
                                <span class="widget-sparkline-title">
                                    <div data-filter=".identity"
                                         class="cbp-filter-item btn btn-circle btn-icon-only green uppercase tooltips"
