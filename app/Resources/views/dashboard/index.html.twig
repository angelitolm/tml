                                <form method="post" action="{{ path('app_admin_social_message_publish') }}">
                                    {% if app.user.userHasFollow(app.user.getActiveProfile.sponsor.user.id) %}
                                        <button type="button" class="btn btn-circle green btn-sm"
                                                onclick="javascript:sponsorFollow('{{ app.user.getActiveProfile.sponsor.user.username }}', 0)">{{ 'unfollow'|trans({},'backend') }}</button>
                                    {% else %}
                                        <button type="button" class="btn btn-circle green btn-sm"
                                                onclick="javascript:sponsorFollow('{{ app.user.getActiveProfile.sponsor.user.username }}', 1)">{{ 'follow'|trans({},'backend') }}</button>
                                    {% endif %}
                                    <button type="button" class="btn btn-circle red btn-sm" data-toggle="modal"
                                            data-target="#messageSponsorModal">{{ 'message'|trans({},'backend') }}</button>
                                </form>
                            </div>
                        {% endif %}
                        <!-- END SIDEBAR BUTTONS -->
                        <!-- SIDEBAR MENU -->
                        <div class="profile-usermenu">
                            <ul class="nav">
                                <li class="active">
                                    <a>
                                        {{ 'code.member' | trans({}, 'backend') }}:
                                    <span class="badge badge-success">
                                        {% if app.user.getActiveProfile.sponsor is empty %}
                                            TML
                                        {% else %}
                                            {{ app.user.getActiveProfile.sponsor.user.code }}
                                        {% endif %}
                                    </span>
                                    </a>
                                </li>
                                <li class="active">
                                    <a>
                                        {{ 'sponsor.email' | trans({}, 'backend') }}: <span class="badge badge-success"
                                                                                            style="text-transform: none">
                                        {% if app.user.getActiveProfile.sponsor == null %}
                                            info@tml4you.com
                                        {% else %}
                                            {{ app.user.getActiveProfile.sponsor.user.email }}
                                        {% endif %}
                                    </span>
                                    </a>
                                </li>
                                <li class="active">
                                    <a style="width: 50.2%; float: left">
                                        {{ 'affiliates' | trans({}, 'backend') }}: <span class="badge badge-success">
                                        {% if app.user.getActiveProfile.sponsor == null %}
                                            0
                                        {% else %}
                                            {{ app.user.getActiveProfile.sponsor.getChildrenMembers|length }}
                                        {% endif %}
                                    </span>
                                    </a>
                                    <a style="border-left: 0; width: 51%; float: left">
                                        {{ 'guests' | trans({}, 'backend') }}: <span class="badge badge-success">
                                        {% if app.user.getActiveProfile.sponsor == null %}
