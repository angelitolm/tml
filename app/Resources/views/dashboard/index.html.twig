                                <li class="active">
                                    <a href="#tab_1_1" data-toggle="tab"> Noticias TML </a>
                                </li>
                                {#<li>#}
                                {#<a href="#tab_1_2" data-toggle="tab"> MLM </a>#}
                                {#</li>#}
                                {#<li>#}
                                {#<a href="#tab_1_3" data-toggle="tab"> Marketing </a>#}
                                {#</li>#}
                                {#<li>#}
                                {#<a href="#tab_1_4" data-toggle="tab"> Others </a>#}
                                {#</li>#}
                            </ul>
                            <div class="tab-content scroller" style="height: 306px;" data-always-visible="1"
                                 data-handle-color="#D7DCE2">
                                <div class="tab-pane fade active in" id="tab_1_1">
                                    <div class="widget-news margin-bottom-20">
                                        {% for news in latestNews %}
                                            {% set url = news.image is not empty ? constant('IMAGES_UPLOAD_DIR',news)~news.image : 'backend/img/photos/news-blur.jpg' %}
