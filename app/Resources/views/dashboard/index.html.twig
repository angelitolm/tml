        <div class="row">
            <div class="col-md-12 margin-bottom-20">
                <!-- BEGIN PORTLET-->
                <div class="profile-sidebar">
                    {% set patrocinador = 'TML-Patrocinador' %}
                    <!-- PORTLET MAIN -->
                    <div class="portlet light profile-sidebar-portlet bordered">
                        <!-- SIDEBAR USERPIC -->
                        <div class="profile-userpic">
                            {% if app.user.getActiveProfile.sponsor == null %}
                                <img class="img-responsive" alt="personal image"
                                     src="{{ asset('bundles/app/images/about/logo-about.jpg') }}"/>
                            {% elseif app.user.getActiveProfile.sponsor.user.picture is not empty %}
                                {% set url = constant('IMAGES_UPLOAD_DIR',app.user.getActiveProfile.sponsor.user)~app.user.getActiveProfile.sponsor.user.picture %}
                                <img class="img-responsive" style="height: 149px; width: 149px" alt="personal image"
                                     src="{{ asset(url) }}"/>
                            {% endif %}
                            {#<img src="../assets/pages/media/profile/profile_user.jpg" class="img-responsive" alt="">#}
                        </div>
                        <!-- END SIDEBAR USERPIC -->
                        <!-- SIDEBAR USER TITLE -->
                        <div class="profile-usertitle">
                            <div class="profile-usertitle-name">
                                {% if app.user.getActiveProfile.sponsor == null %}
                                    <a href="">{{ patrocinador }}</a>
                                {% else %}
                                    <a href="">{{ app.user.getActiveProfile.sponsor.user }}</a>
                                {% endif %}
                            </div>
                            <div class="profile-usertitle-job">
                                {% if app.user.getActiveProfile.sponsor == null %}
                                    CEO
                                {% elseif app.user.getActiveProfile.sponsor.user.hasRole('ROLE_PROFESSIONAL') %}
                                    PROFESIONAL
                                {% elseif app.user.getActiveProfile.sponsor.user.hasRole('ROLE_SUPERIOR') %}
                                    SUPERIOR
                                {% else %}
                                    BASICO
                                {% endif %}
                            </div>
                        </div>
                        <!-- END SIDEBAR USER TITLE -->
                        <!-- SIDEBAR BUTTONS -->
                        {% if app.user.getActiveProfile.sponsor is not empty %}
