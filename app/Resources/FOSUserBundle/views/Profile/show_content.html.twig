<div class="modal fade" id="comemberModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"
     aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                <h4 class="modal-title">{{ 'edit.comember' | trans({}, 'backend') }}</h4>
            </div>
            <div class="modal-body padT10">
                <form class="form-horizontal" method="post" action="{{ path('app_admin_user_edit_comember') }}"
                      id="comember">

                    <div class="form-group">
                        <label class="col-md-3 control-label">{{ 'name_comembre' | trans({}, 'backend') }}</label>

                        <div class="col-md-9">
                            <div class="input-group">
                               <span class="input-group-addon">
                               <i class="fa fa-user"></i>
                               </span>
                                <input name="app_comember[name]" class="form-control" placeholder="{{ 'name_comembre' | trans({}, 'backend') }}" type="text" value="{% if user.comember is not empty %}{{ user.comember.name }}{% endif %}">
                            </div>
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="col-md-3 control-label">{{ 'mail_comembre' | trans({}, 'backend') }}</label>

                        <div class="col-md-9">
                            <div class="input-group">
                               <span class="input-group-addon">
                               <i class="fa fa-envelope"></i>
                               </span>
                                <input name="app_comember[mail]" class="form-control" placeholder="{{ 'mail_comembre' | trans({}, 'backend') }}" type="email" value="{% if user.comember is not empty %}{{ user.comember.mail }}{% endif %}">
                            </div>
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="col-md-3 control-label">{{ 'addres_comembre' | trans({}, 'backend') }}</label>

                        <div class="col-md-9">
                            <div class="input-group">
                               <span class="input-group-addon">
                               <i class="fa fa-map-marker"></i>
                               </span>
                                <input name="app_comember[address]" class="form-control" placeholder="{{ 'address_comembre' | trans({}, 'backend') }}" type="text" value="{% if user.comember is not empty %}{{ user.comember.address }}{% endif %}">
                            </div>
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="col-md-3 control-label">{{ 'phone_comembre' | trans({}, 'backend') }}</label>

                        <div class="col-md-9">
                            <div class="input-group">
                               <span class="input-group-addon">
                               <i class="fa fa-phone"></i>
                               </span>
                                <input name="app_comember[phone]" class="form-control" placeholder="{{ 'phone_comembre' | trans({}, 'backend') }}" type="text" value="{% if user.comember is not empty %}{{ user.comember.phone }}{% endif %}">
                            </div>
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="col-md-3 control-label">{{ 'dni_comembre' | trans({}, 'backend') }}</label>

                        <div class="col-md-9">
                            <div class="input-group">
                               <span class="input-group-addon">
                               <i class="fa fa-credit-card"></i>
                               </span>
                                <input name="app_comember[dni]" class="form-control" placeholder="{{ 'dni_comembre' | trans({}, 'backend') }}" type="text"  value="{% if user.comember is not empty %}{{ user.comember.dni }}{% endif %}">
                            </div>
                        </div>
                    </div>

                    <div class="button-group text-right">
                        <button type="button" class="btn black"
                                data-dismiss="modal">{{ 'close'|trans({},'resources') }}</button>
                        <button type="submit" class="btn green">{{ 'send'|trans({},'resources') }}</button>
                    </div>

                </form>

            </div>

        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>
