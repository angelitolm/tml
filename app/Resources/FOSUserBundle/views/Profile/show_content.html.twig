                                <div class="scroller" style="height: 230px;"
                                     data-always-visible="1"
                                     data-handle-color="#D7DCE2">
                                    <table class="table table-hover table-light">
                                        <thead>
                                        <tr class="uppercase">
                                            <th colspan="2">
                                                {{ 'members' | trans({}, 'backend') }}
                                            </th>
                                            <th>
                                                {{ 'mail' | trans({}, 'backend') }}
                                            </th>
                                            <th>
                                                {{ 'membership' | trans({}, 'backend') }}
                                            </th>
                                        </tr>
                                        </thead>

                                        <tbody>
                                        {% for affiliate in app.user.getActiveProfile.getChildrenMembers %}
                                            <tr>

                                                <td class="fit">
                                                    {% if affiliate.user.picture == null %}
                                                        <img class="user-pic" alt=""
                                                             src="{{ asset('backend-metronic/img/avatars/avatar.jpg') }}"/>
                                                    {% elseif affiliate.user.picture is not empty %}
                                                        {% set url = constant('IMAGES_UPLOAD_DIR',affiliate.user)~affiliate.user.picture %}
                                                        <img class="user-pic" alt="" src="{{ asset(url) }}"/>
                                                    {% endif %}
                                                    {#<img class="user-pic" src="{{ affiliate.user.picture }}">#}
                                                </td>
                                                <td>
                                                    <a href="javascript:;"
                                                       class="primary-link">{{ affiliate.user.name }}</a>
                                                </td>
                                                <td>
                                                    <a href="javascript:;"
                                                       class="primary-link">{{ affiliate.user.email }}</a>
                                                </td>
                                                <td>
                                                    {% if affiliate.user == null %}
                                                        CEO
                                                    {% elseif affiliate.user.hasRole('ROLE_PROFESSIONAL') %}
                                                        PROFESIONAL
                                                    {% elseif affiliate.user.hasRole('ROLE_SUPERIOR') %}
                                                        SUPERIOR
                                                    {% else %}
                                                        BASICO
                                                    {% endif %}
                                                </td>

                                            </tr>
                                        {% endfor %}
                                        </tbody>

                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    {#<!-- END PORTLET -->#}

                </div>


